/**index.wxss**/
.com-photo-ul {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 144rpx;
  background:rgba(46,51,74,1);
  box-shadow:0px 2rpx 10rpx 0px rgba(0,0,0,0.5);
  border-radius:20rpx;
  margin: 24rpx 0;
}

.com-photo-left{
  width: 95%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.photo-img{
  width: 84rpx;
  height: 84rpx;
  border-radius: 42rpx;
  margin-left:40rpx;
}

.photo-name{
  display: flex;
  flex-direction: column;
  margin-left: 40rpx;
}
.photo-title{
  font-size:28rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:40rpx;
}

.photo-time{
  font-size:26rpx;
  font-weight:400;
  color:rgba(151,153,164,1);
  line-height:37rpx;
}

.com-photo-right{
  width: 5%;
  display: flex;
  align-items: center;
}

.photo-arrow{
  width: 10rpx;
  height: 20rpx;
}
