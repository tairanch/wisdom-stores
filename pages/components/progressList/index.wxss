/**index.wxss**/
.com-progress-list{
  display: flex;
  flex-direction: column;
}

.com-progress-ul{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 15rpx 0;
}

.com-progress-name{
  width: 46%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;

  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:30rpx;
}

.com-progress-progress{
  width: 40%;
  margin: 0 2%;
}

.com-progress-score{
  width: 10%;
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:26rpx;
  text-align: right;
}
